{
  "version": 2,
  // 定义项目的构建步骤
  "builds": [
    {
      // 源文件路径，指定要构建的文件或目录
      "src": "src/index.js",
      // 构建器，指定使用的构建工具或运行环境
      "use": "@vercel/node"
    }
  ],
  // 定义项目的路由规则，控制请求如何被转发到不同的文件或路径。
  "routes": [
    {
      // 匹配的路径模式，使用正则表达式
      "src": "/(.*)",
      // 请求匹配后要转发的目标路径。
      "dest": "src/index.js"
    }
  ]
}
